<view class="detail-container"><view class="company-card"><view class="company-header"><text class="company-name">{{a}}</text></view><view class="company-tags"><text wx:if="{{b}}" class="tag">{{c}}</text><text wx:if="{{d}}" class="tag">{{e}}</text><text wx:if="{{f}}" class="tag">{{g}}</text></view><view wx:if="{{h}}" class="company-desc"><text class="section-title">公司简介</text><text class="desc-text">{{i}}</text></view></view><view class="job-section"><view class="section-header"><text class="section-title">在招职位</text><text class="job-count">({{j}}个)</text></view><view class="job-list"><view wx:for="{{k}}" wx:for-item="job" wx:key="j" class="job-item" bindtap="{{job.k}}"><view class="job-header"><text class="job-title">{{job.a}}</text><text class="job-salary">{{job.b}}</text></view><view class="job-tags"><text wx:if="{{job.c}}" class="tag">{{job.d}}</text><text wx:if="{{job.e}}" class="tag">{{job.f}}</text><text wx:if="{{job.g}}" class="tag">{{job.h}}</text></view><view class="job-desc">{{job.i}}</view></view></view><view wx:if="{{l}}" class="load-more" bindtap="{{m}}"><text>加载更多</text></view><view wx:else class="no-more"><text>没有更多了</text></view></view></view>