<view class="collection-container"><view wx:if="{{a}}" class="login-tip"><text>请先登录</text><button class="login-btn" bindtap="{{b}}">去登录</button></view><block wx:else><view class="job-list"><view wx:for="{{c}}" wx:for-item="job" wx:key="h" class="job-item" bindtap="{{job.i}}"><view class="job-header"><text class="job-title">{{job.a}}</text><text class="job-salary">{{job.b}}</text></view><view class="company-info"><text class="company-name">{{job.c}}</text><text class="job-location">{{job.d}}</text></view><view class="job-tags"><text wx:for="{{job.e}}" wx:for-item="tag" wx:key="b" class="tag">{{tag.a}}</text></view><view class="job-footer"><text class="collect-time">收藏于 {{job.f}}</text><button class="cancel-btn" catchtap="{{job.g}}">取消收藏</button></view></view></view><view wx:if="{{d}}" class="load-more" bindtap="{{e}}"><text>加载更多</text></view><view wx:else class="no-more"><text>没有更多了</text></view><view wx:if="{{f}}" class="empty-state"><text>暂无收藏</text></view></block></view>